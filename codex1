#include<stdio.h>
#include<string.h>

int main()
{
    char str1[50] = "My Name is ";

    char str2[] = "Raihan Wahi";

    int ln=0 ,i=0,j=0;

    while(str1[i]!= '\0')
    {
        i++;
        ln++;
    }
    while(str2[j]!='\0')
    {
        str1[ln+j]=str2[j];

        j++;
    }
    printf("%s\n",str1);

    return 0;
}
